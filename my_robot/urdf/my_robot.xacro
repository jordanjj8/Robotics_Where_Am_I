<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo"/>
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
      <material name="light blue">
  <color rgba="0.35 0.61 0.87 1.0"/>
      </material>
    </visual>
  </link>

  <link name="left_rearwheel">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
  
  <collision name="collision">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
  </collision>  
  
  <visual name="left_rearwheel_visual">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
  </visual>
  </link>

  <link name="left_frontwheel">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
  
  <collision name="collision">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
  </collision>  
  
  <visual name="left_frontwheel_visual">
    <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
    <geometry>
      <cylinder radius="0.1" length="0.1"/>
    </geometry>
  </visual>
  </link>

  <link name="right_rearwheel">
      <inertial>
            <mass value="5.0"/>
            <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
            <inertia
               ixx="0.1" ixy="0" ixz="0"
               iyy="0.1" iyz="0"
               izz="0.1"
            />
        </inertial>

    <collision name="collision">
         <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
         <geometry>
            <cylinder radius="0.1" length="0.1"/>
         </geometry>
      </collision>  
  
      <visual name="right_rearwheel_visual">
         <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
         <geometry>
           <cylinder radius="0.1" length="0.1"/>
         </geometry>
      </visual>
  </link>

  <link name="right_frontwheel">
      <inertial>
            <mass value="5.0"/>
            <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
            <inertia
               ixx="0.1" ixy="0" ixz="0"
               iyy="0.1" iyz="0"
               izz="0.1"
            />
        </inertial>

    <collision name="collision">
         <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
         <geometry>
            <cylinder radius="0.1" length="0.1"/>
         </geometry>
      </collision>  
  
      <visual name="right_frontwheel_visual">
         <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
         <geometry>
           <cylinder radius="0.1" length="0.1"/>
         </geometry>
      </visual>
  </link>

  <joint name="left_rearwheel_hinge" type="continuous">
    <origin xyz="-0.15 0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="left_rearwheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="right_rearwheel_hinge" type="continuous">
    <origin xyz="-0.15 -0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="right_rearwheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="left_frontwheel_hinge" type="continuous">
    <origin xyz="0.15 0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="left_frontwheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint name="right_frontwheel_hinge" type="continuous">
    <origin xyz="0.15 -0.15 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="right_frontwheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>  

  <link name="camera">
    <inertial>
    <mass value="0.1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <inertia
      ixx="1e-6" ixy="0" ixz="0"
      iyy="1e-6" iyz="0" 
      izz="1e-6"
    />
    <box_inertia m="0.1" x="0.05" y="0.05" z="0.05"/>
    </inertial>
    
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
    
    <visual name="camera visual">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
  </link>
  
  <joint name="camera_joint" type="fixed">
    <origin xyz="0.2 0 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="camera"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>
  
  <link name="hokuyo">
    <interial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
        ixx="1e-6" ixy="0" ixz="0"
        iyy="1e-6" iyz="0" 
        izz="1e-6"  
      />
    </interial>

    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
  </link>

  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0.15 0 0.1" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="hokuyo"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>


</robot>
